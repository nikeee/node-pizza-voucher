{
	"compilerOptions": {
		"module": "commonjs",
		"declaration": false,
		"target": "es2015",
		"sourceMap": false,
		"outDir": "bin",
		"newLine": "LF",
		"removeComments": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"strictNullChecks": true,
		"alwaysStrict": true
	}
}
